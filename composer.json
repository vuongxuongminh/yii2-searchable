{
    "name": "vxm/yii2-search",
    "description": "Simple full-text search for Yii2 active record",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","full-text-search","active-record"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Vuong Minh",
            "email": "vuongxuongminh@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "yiisoft/yii2": ">=2.0.14.2",
        "yiisoft/yii2-queue": "^2.2",
        "teamtnt/tntsearch": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "vxm\\search\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "bootstrap": "vxm\\search\\Bootstrap"
    }
}
